{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "verPluginJSON": {
          "allOf": [
            { "type": "string" },
            { "maxLength": 15 }
          ]
        },
        "verBMSJSON": {
          "allOf": [
            { "type": "string" },
            { "maxLength": 15 }
          ]
        },
        "userField": {
          "allOf": [
            { "type": "string" },
            { "maxLength": 250 }
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "verPluginJSON",
        "verBMSJSON"
      ]
    }
  ],
  "additionalItems": {
    "type": "object",
    "properties": {
      "menuTarget": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 30 }
        ]
      },
      "targetPhrases": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 100 }
        ]
      },
      "menuName": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 30 }
        ]
      },
      "menuNamePhrases": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 100 }
        ]
      },
      "directMenuGroup": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 50 }
        ]
      },
      "menuShow": {
        "allOf": [
          { "type": "array" },
          {
            "items": {
              "type": "string",
              "maxLength": 40
            }
          },
          { "minItems": 1 },
          { "maxItems": 15 }
        ]
      },
      "menuItems": {
        "type": "array",
        "items": {
          "allOf": [
            { "type": "array" },
            { "minItems": 2 },
            { "maxItems": 3 },
            {
              "items": {
                "allOf": [
                  { "type": "string" },
                  { "maxLength": 300 }
                ]
              }
            }
          ]
        }
      }
    },
    "additionalProperties": false,
    "required": [
      "menuTarget",
      "targetPhrases",
      "menuName",
      "menuNamePhrases",
      "directMenuGroup",
      "menuShow",
      "menuItems"
    ]
  }
}