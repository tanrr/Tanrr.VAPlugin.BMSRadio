{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "menuTarget": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 30 }
        ]
      },
      "targetPhrases": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 100 }
        ]
      },
      "menuName": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 30 }
        ]
      },
      "menuNamePhrases": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 100 }
        ]
      },
      "menuShow": {
        "allOf": [
          { "type": "string" },
          { "maxLength": 30 }
        ]
      },
      "menuItems": {
        "type": "array",
        "items": {
          "allOf": [
            { "type": "array" },
            { "minItems": 2 },
            { "maxItems": 3 },
            {
              "items": {
                "allOf": [
                  { "type": "string" },
                  { "maxLength": 150 }
                ]
              }
            }
          ]
        }
      }
    },
    "required": [
      "menuTarget",
      "targetPhrases",
      "menuName",
      "menuNamePhrases",
      "menuShow",
      "menuItems"
    ]
  }
}